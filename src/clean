rm -rf gdb
rm -rf gdk-pixbuf
rm -rf gettext
rm -rf glib
rm -rf gmp
rm -rf gobject-introspection
rm -rf gtk+
rm -rf guile
rm -rf harfbuzz
rm -rf icu
rm -rf jpeg
rm -rf json-c
rm -rf libarchive
rm -rf libatomic_ops
rm -rf libffi
rm -rf libpng
rm -rf libtool
rm -rf libunistring
rm -rf libxml2
rm -rf libxslt
rm -rf mpc
rm -rf mpfr
rm -rf OpenBLAS
rm -rf openmpi
rm -rf openssl
rm -rf pango
rm -rf pcre
